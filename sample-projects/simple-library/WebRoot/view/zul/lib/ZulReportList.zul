			<tabs>
				<tab label="Report" />
			</tabs>
			<tabpanels>
				<tabpanel>
					<grid sclass="input-field">
						<columns>
							<column width="100px" />
							<column />
						</columns>
						<rows>
							<row>
								<label id="xReport" value="Report" />
								<hbox align="center">
									<combobox id="cmbReport"
										width="150px" />
