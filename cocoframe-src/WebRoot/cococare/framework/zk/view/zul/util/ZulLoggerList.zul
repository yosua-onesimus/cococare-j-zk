			<tabs>
				<tab label="" />
			</tabs>
			<tabpanels>
				<tabpanel>
					<borderlayout>
						<north>
							<hbox align="center" hflex="1">
								<combobox id="cmbUsername"
									tooltiptext="Username" width="100px" readonly="true" />
								<combobox id="cmbScreen"
									tooltiptext="Screen" width="150px" readonly="true" />
								<combobox id="cmbAction"
									tooltiptext="Action" width="100px" readonly="true" />
